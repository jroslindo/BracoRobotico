<AnchorPane id="AnchorPane" prefHeight="267.0" prefWidth="320" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="principal.FXMLDocumentController">
    <children>
        <Label fx:id="label" layoutX="80.0" layoutY="22.0" minHeight="16" minWidth="69" text="Calculadora de talentos WOW">
