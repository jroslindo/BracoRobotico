      <TextField fx:id="Valor" layoutX="127.0" layoutY="60.0" prefHeight="25.0" prefWidth="94.0" />
      <Label layoutX="25.0" layoutY="103.0" text="Classe:" />
      <CheckBox fx:id="Check1" layoutX="72.0" layoutY="103.0" mnemonicParsing="false" text="Demon Hunter" />
      <CheckBox fx:id="Check2" layoutX="185.0" layoutY="103.0" mnemonicParsing="false" text="Death Knight" />
      <CheckBox fx:id="Check3" layoutX="72.0" layoutY="134.0" mnemonicParsing="false" text="Priest" />
      <CheckBox fx:id="Check4" layoutX="185.0" layoutY="134.0" mnemonicParsing="false" text="Rogue" />
      <Button fx:id="Calcular" layoutX="122.0" layoutY="168.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Calcular" />
      <Label layoutX="25.0" layoutY="207.0" text="A quantidade de talentos disnponíveis é: " />
      <Label fx:id="Resultado" layoutX="249.0" layoutY="207.0" />
    </children>
</AnchorPane>
